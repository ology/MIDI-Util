use lib $ENV{HOME} . '/sandbox/MIDI-Util/lib';
use MIDI::Util qw(midi_dump);
use Data::Dumper::Compact qw(ddc);

$Data::Dumper::Varname = 'Length';
print ddc(midi_dump('length'));

